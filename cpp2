#include <iostream>
using namespace std;

int main() {
    int t,n;
    long int x,a,y;
    cin>>t;
    for(int i=0;i<t;i++){
        cin>>n;
        
            cin>>x;
            for(int i=0;i<n;i++){
                cin>>a;
                if(a>=x)
                {
                    cout<<"YES"<<endl;
                    y=1;
                    break;
                    
                }
         }
       
  if( y==0){
      cout<<"NO"<<endl;
      
  }     
      y=0;  }
    
	return 0;
}
